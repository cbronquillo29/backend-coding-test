{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha tests",
    "start": "node index.js",
    "lint": "eslint src/*.js",
    "coverage:report": "nyc --check-coverage --lines 80 --statements 80 --branch 80 --per-file --reporter=html mocha tests"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.0.6",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "mocha": "^6.1.4",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  }
}
